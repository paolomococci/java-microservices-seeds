<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>


<AnchorPane fx:id="mainAnchorPane" 
            id="AnchorPane" 
            prefHeight="600.0" 
            prefWidth="400.0" 
            styleClass="main-sample-two-view" 
            stylesheets="@../style/sample-two-view.css" 
            xmlns="http://javafx.com/javafx/11.0.1" 
            xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="local.example.seed.controller.SampleTwoViewController">
    <children>
        <GridPane fx:id="gridPane" 
                  prefHeight="600.0" 
                  prefWidth="400.0" 
                  AnchorPane.bottomAnchor="0.0" 
                  AnchorPane.leftAnchor="0.0" 
                  AnchorPane.rightAnchor="0.0" 
                  AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" 
                                   minWidth="10.0" 
                                   prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="50.0" 
                                minHeight="10.0" 
                                prefHeight="50.0" 
                                vgrow="SOMETIMES" />
                <RowConstraints maxHeight="500.0" 
                                minHeight="10.0" 
                                prefHeight="500.0" 
                                vgrow="SOMETIMES" />
                <RowConstraints maxHeight="50.0" 
                                minHeight="10.0" 
                                prefHeight="50.0" 
                                vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <AnchorPane styleClass="header">
                    <Label fx:id="labelHead" 
                           text="sample two" 
                           styleClass="label" 
                           GridPane.rowIndex="0" />
                </AnchorPane>
                <Pane fx:id="mainPane" 
                      prefHeight="500.0" 
                      prefWidth="400.0" 
                      styleClass="pane" 
                      GridPane.rowIndex="1" />
                <ButtonBar fx:id="buttonBar" 
                           prefHeight="45.0" 
                           prefWidth="400.0" 
                           styleClass="button-bar" 
                           GridPane.rowIndex="2">
                    <buttons>
                        <Button fx:id="goButton" 
                                mnemonicParsing="false" 
                                styleClass="button" 
                                onAction="#go" 
                                text="go" />
                    </buttons>
                </ButtonBar>
            </children>
        </GridPane>
    </children>
</AnchorPane>
